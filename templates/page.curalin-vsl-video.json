      "settings": {
        "custom_liquid": "<script>\n        // Format time in MM:SS\nfunction formatTime(milliseconds) {\n  const totalSeconds = Math.floor(milliseconds / 1000);\n  const minutes = Math.floor(totalSeconds / 60);\n  const seconds = totalSeconds % 60;\n  return `${minutes}:${seconds.toString().padStart(2, '0')}`;\n}\n\n// Function to get URL parameters\nfunction getUrlParameter(name) {\n  const params = new URLSearchParams(window.location.search);\n  return params.get(name);\n}\n\n// Function to set cookie\nfunction setCookie(name, value, days) {\n  const date = new Date();\n  date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));\n  const expires = \"expires=\" + date.toUTCString();\n  document.cookie = name + \"=\" + value + \"; path=/; \" + expires;\n  console.log(`Cookie set: ${name}=${value}, expires in ${days} days`);\n}\n\n// Function to get cookie\nfunction getCookie(name) {\n  const nameEQ = name + \"=\";\n  const ca = document.cookie.split(';');\n  for(let i = 0; i < ca.length; i++) {\n    let c = ca[i];\n    while (c.charAt(0) === ' ') c = c.substring(1, c.length);\n    if (c.indexOf(nameEQ) === 0) {\n      const value = c.substring(nameEQ.length, c.length);\n      console.log(`Cookie found: ${name}=${value}`);\n      return value;\n    }\n  }\n  console.log(`Cookie not found: ${name}`);\n  return null;\n}\n\n// Function to show all full elements\nfunction showFullElements() {\n  const fullElements = document.querySelectorAll('.full');\n  console.log(`Showing ${fullElements.length} full elements`);\n  fullElements.forEach(element => {\n    element.style.display = 'block';\n  });\n}\n\n// Function to handle URL updates\nfunction updateUrlWithFullParam() {\n  const currentParams = new URLSearchParams(window.location.search);\n  if (!currentParams.has('full')) {\n    currentParams.set('full', 'true');\n    const newUrl = `${window.location.pathname}?${currentParams.toString()}`;\n    window.history.replaceState({}, '', newUrl);\n    console.log('URL updated with full parameter');\n  }\n}\n\n// Function to get remaining time\nfunction getRemainingTime(startTime) {\n  const THIRTY_THREE_MINUTES = 33 * 60 * 1000;\n  const elapsedTime = Date.now() - startTime;\n  const remainingTime = Math.max(0, THIRTY_THREE_MINUTES - elapsedTime);\n  console.log(`Time elapsed: ${formatTime(elapsedTime)}, Remaining: ${formatTime(remainingTime)}`);\n  return remainingTime;\n}\n\n// Main logic\ndocument.addEventListener('DOMContentLoaded', function() {\n  console.log('Page loaded, initializing timer logic');\n\n  const hasSeenContent = getCookie('hasSeenContent');\n  const hasFullParam = getUrlParameter('full') === 'true';\n  const startTime = localStorage.getItem('pageStartTime');\n\n  console.log('Initial state:', {\n    hasSeenContent,\n    hasFullParam,\n    startTime: startTime ? new Date(parseInt(startTime)).toLocaleString() : null\n  });\n\n  if (hasSeenContent === 'true' || hasFullParam) {\n    console.log('Content already seen or full parameter present, showing content immediately');\n    showFullElements();\n    updateUrlWithFullParam();\n  } else {\n    const currentTime = Date.now();\n\n    if (!startTime) {\n      console.log('First visit, starting 33-minute timer');\n      localStorage.setItem('pageStartTime', currentTime.toString());\n      setTimeout(() => {\n        console.log('Timer complete, showing content');\n        showFullElements();\n        updateUrlWithFullParam();\n        setCookie('hasSeenContent', 'true', 90);\n      }, 33 * 60 * 1000);\n    } else {\n      const remainingTime = getRemainingTime(parseInt(startTime));\n\n      if (remainingTime <= 0) {\n        console.log('Timer already expired, showing content immediately');\n        showFullElements();\n        updateUrlWithFullParam();\n        setCookie('hasSeenContent', 'true', 90);\n      } else {\n        console.log(`Resuming timer with ${formatTime(remainingTime)} remaining`);\n        setTimeout(() => {\n          console.log('Timer complete, showing content');\n          showFullElements();\n          updateUrlWithFullParam();\n          setCookie('hasSeenContent', 'true', 90);\n        }, remainingTime);\n      }\n    }\n  }\n\n});\n      </script>\n\n<style>\nsection.full {\n display: none !important; \n}\n</style>",
